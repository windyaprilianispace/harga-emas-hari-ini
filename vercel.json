{
  "version": 2,
  "builds": [
    {
      "src": "api/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/gold-prices",
      "dest": "/api/gold-prices.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
